# Cyzor Provisioning - Environment Variables
# Copy this file to .env and replace the values with your actual configuration

# .NET Environment
ASPNETCORE_ENVIRONMENT=Production

# Webhook Configuration
CYZOR_WEBHOOK_SECRET=your_webhook_secret_key_here

# Slack Integration (Optional)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# Provisioning Service Configuration
Provisioning__Host=72.60.247.117
Provisioning__User=root
Provisioning__Password=your_password_here
Provisioning__AppType=node
Provisioning__BuildsBasePath=/var/www/builds
Provisioning__AppsBasePath=/var/www

# Logging Configuration (Optional)
Logging__LogLevel__Default=Information
Logging__LogLevel__Microsoft.Hosting.Lifetime=Information

# Database Connection (If applicable)
# ConnectionStrings__DefaultConnection=Server=localhost;Database=cyzor;User Id=sa;Password=your_password;
